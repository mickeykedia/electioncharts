.header_title {
	display:inline;
}

.header_party_image {
	height:70px;
	width:70px;
}

.circle {
    width:200px;
    height:200px;
    border-radius:100px;
    font-size:30px;
    color:#ffffff;
    line-height:200px;
    text-align:center;
    margin:0px auto 0px auto;
    background:#0D8FDB

}

.bar.postive {
	fill:steelblue;
}

.bar.negative {
	fill:brown;
}
.axis text {
	font: 10px sans-serif;
}
.axis path,
.axis line {
  fill: none;
	stroke: #000;
  shape-rendering: crispEdges;
}

rect:hover {
    opacity: 0.5;
}

.gridStyle {
    border: 1px solid rgb(212,212,212);
    width: 100%; 
    height: 400px;
}
