<div >
    <h3>Election Charts</h3>
    <ul class="nav nav-tabs">
        <li ><a ng-href="/">Home</a></li>
        <li class="dropdown ">
            <a dropdown-toggle data-toggle="dropdown" >
                Parties <span class="caret"></span>
            </a>
            <ul class="dropdown-menu" role="menu">
                <li><a ng-href="/party/1"> BJP</a></li>
                <li><a ng-href="/party/2"> Congress </a></li>
                <li><a ng-href="/party/371"> AAP </a></li>
                <li><a ng-href="/party/248"> AIADMK </a></li>
                <li><a ng-href="/party/3"> BSP </a></li>
                <li><a ng-href="/party/247"> DMK </a></li>
                <li><a ng-href="/party/34"> JD(U) </a></li>
                <li><a ng-href="/party/49"> NCP </a></li>
                <li><a ng-href="/party/16"> SP </a></li>
                <li><a ng-href="/party"> All Parties </a></li>
            </ul>
        </li>
        <li class="dropdown">
            <a dropdown-toggle data-toggle="dropdown" >
                Coalitions <span class="caret"></span>
            </a>
            <ul class="dropdown-menu">
                <li><a ng-href="/coalition/2"> BJP +</a></li>
                <li><a ng-href="/coalition/1"> Congress +</a></li>
            </ul>
        </li>
        <li ><a ng-href="/state">States</a></li>
        <li ><a ng-href="/constituency">Constituencies</a></li>
        <li class="active"><a ng-href="/candidate">Candidates</a></li>
    </ul>
    <ol class="breadcrumb breadcrumb-peer">
        <li class="active">Lok Sabha 2014, Live Results </li>
    </ol>
    <div class="page-header">
        <h1 class="header-title">Candidate List</h1>
    </div>
  <div class="input-group">
        <span class="input-group-addon">Search</span>
        <input type="text" class="form-control" data-ng-model="filtertext" placeholder="Filter by Candidate/Constituency/Party">
    </div>
  <table class='table table-hover'>
    <tr>
      <th class="col-md-2">Candidate</th>
      <th class="col-md-2">Constituency</th>
      <th class="col-md-2">Party</th>
    </tr>
    
    <tr ng-repeat="candidate in candidateList| filter:filtertext">
      <td><a ng-href="/candidate/{{candidate.candidate_id}}">{{candidate.candidate_name | titlecase}}</a></td>
      <td><a ng-href="/constituency/{{candidate.constituency_id}}">{{candidate.constituency_name | titlecase}}</a></td>
      <td><a ng-href="/party/{{candidate.party_id}}">{{candidate.party_name|titlecase}}</a></td>
    </tr>
  </table>
</div>
