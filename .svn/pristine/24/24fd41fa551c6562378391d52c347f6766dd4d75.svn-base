<div class="container" ng-controller="StateCtrl">
  <div class="well">
    <div class="row">
      <div class="col-md-6">State Name: {{state.name}}</div>
      <div class="col-md-6">State ID: {{state.id}}</div>
    </div>
  </div>
  <table class="table table-hover">
    <tr>
      <th class="col-md-3">Party Name</th>
      <th class="col-md-1">Wins</th>
      <th class="col-md-1">Leads</th>
      <th class="col-md-1">Votes</th>
    </tr>
    
    <tr ng-repeat="result in results">
      <td><a ng-href="/party/{{result.party_id}}"> {{result.party_name}}</a></td>
      <td>{{result.wins}}</td>
      <td>{{result.leads}}</td>
      <td>{{result.votes}}</td>
    </tr> 
  </table>
</div>
